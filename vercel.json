{
    "buildCommand": "npm run vercel-build",
    "outputDirectory": "dist",
    "framework": null,
    "cleanUrls": true,
    "rewrites": [
      { "source": "/api/(.*)", "destination": "/api/$1" },
      { "source": "/share/(.*)", "destination": "/api/share?handle=$1" },
      { "source": "/(.*)", "destination": "/" }
    ]
  }
  