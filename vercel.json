{
    "buildCommand": "npm run vercel-build",
    "outputDirectory": "dist",
    "framework": null,
    "cleanUrls": true,
    "builds": [
      { "src": "dist/**/*", "use": "@vercel/static" },
      { "src": "api/**/*.ts", "use": "@vercel/node" }
    ],
    "routes": [
      { "src": "/api/(.*)", "dest": "/api/$1.ts" },
      { "src": "/share/(.*)", "dest": "/api/share.ts?handle=$1" },
      { "src": "/(.*)", "dest": "/dist/$1" }
    ]
  }
  