{
    "buildCommand": "npm run vercel-build",
    "outputDirectory": "dist",
    "framework": null,
    "cleanUrls": true,
    "rewrites": [
      { "source": "/api/(.*)", "destination": "/api/$1.ts" },
      { "source": "/share/(.*)", "destination": "/api/share.ts?handle=$1" },
      { "source": "/(.*)", "destination": "/dist/$1" }
    ]
  }
  